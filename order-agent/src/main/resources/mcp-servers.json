{
  "mcpServers": {
    "mcp-server-order": {
      "command": "/Library/Java/JavaVirtualMachines/jdk-21.0.3.jdk/Contents/Home/bin/java",
      "args": [
        "-Dspring.ai.mcp.server.stdio=true",
        "-jar",
        "/Users/renhd/work/eclipsews/spring-ai-mcp-demo/mcp-server-order/target/mcp-server-order-0.0.1-SNAPSHOT.jar"
      ]
    },
    "mcp-server-alipay": {
      "command": "npx",
      "args": ["-y", "@alipay/mcp-server-alipay"],
      "env": {
        "AP_APP_ID": "${AP_APP_ID}",
        "AP_APP_KEY": "${AP_APP_KEY}",
        "AP_PUB_KEY": "${AP_PUB_KEY}",
        "AP_CURRENT_ENV": "sandbox",
        "AP_RETURN_URL": "http://localhost:8080/callback"
      }
    }
  }
}